/*
 *
 * Index: `{{{ PROJECT_NAME_SPACE }}}/{{{ name }}}-app`.
 *
*/
import renderAppWithRedBox from "{{{ PROJECT_NAME_SPACE }}}/app-with-redbox";
import App from "{{{ PROJECT_NAME_SPACE }}}/base-app-wrapper";
import pagesRoutesData from "./pagesRoutesData";

renderAppWithRedBox(<App routesData={pagesRoutesData} />);

if ("serviceWorker" in navigator) {
  window.addEventListener("load", () => {
    navigator.serviceWorker
      .register("/service-worker.js")
      .then((registration) => {
        console.log("SW registered: ", registration);
      })
      .catch((registrationError) => {
        console.log("SW registration failed: ", registrationError);
      });
  });
}
