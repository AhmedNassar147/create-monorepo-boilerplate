{
  "name": "create-react-monorepo-boilerplate",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "app",
    "packages/*",
    "some-module/*",
    "lorem-ipsum-app"
  ],
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "clean-locks": "rm -rf ./yarn.lock",
    "bootstrap": "lerna bootstrap",
    "bootstrap:packages": "lerna bootstrap",
    "reset": "yarn clean && yarn clean-locks && yarn bootstrap",
    "check:bins": "node ./devpackages/install-bins/src/index.js",
    "postbootstrap": "yarn check:bins && yarn ts-check-packages && yarn build-packages",
    "prettify": "prettier --write",
    "generate": "plop --plopfile ./devpackages/generators/src/index.js",
    "generate-routes": "plop --plopfile ./devpackages/generators/src/build-app-routes/index.js",
    "ts-check-packages": "lerna run --parallel ts:checker:declaration --",
    "build-packages": "lerna run --parallel prepare --",
    "start:js": "webpack serve --config ./devpackages/webpack --node-env=development",
    "validate-dev-assets": "domain-validate-app-assets --mode=development",
    "start": "stmux -- [ [ -s 1/3 \"yarn ts-check-packages --watch\" .. \"yarn build-packages --watch\" ] : \"yarn start:js\" ]",
    "prebuild": "domain-validate-app-assets --mode=production && yarn build-packages --configEnvName production",
    "build": "webpack --config ./devpackages/webpack --node-env=production --analyze",
    "// postbuild": "domain-post-whatsapp-build-message",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json,md}\"",
    "lint:modified": "git add . && domain-precommit-linter",
    "lint:eslint": "eslint",
    "lint:js:fix": "NODE_OPTIONS='--max-old-space-size=5700' npm run lint:eslint  -- . --rule 'no-debugger: 2' --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@domain/app-with-redbox": "1.0.0",
    "@domain/base-app-wrapper": "1.0.0",
    "@domain/base-page": "1.0.0",
    "@domain/jsx-icons": "1.0.0",
    "@domain/labels-provider": "1.0.0",
    "@domain/loader-fallback": "1.0.0",
    "@domain/lorem-ipsum-page": "1.0.0",
    "@domain/react-lazy": "1.0.0",
    "@domain/routes": "1.0.0",
    "@domain/some-new-page": "1.0.0",
    "@domain/types": "1.0.0",
    "preact": "^10.5.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-define": "^2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-minify": "^0.5.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^4.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cors": "^2.8.5",
    "css-loader": "^6.1.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "form-data": "^4.0.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "lerna": "^4.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "redbox-react": "^1.6.0",
    "rollup": "^2.53.2",
    "stmux": "^1.8.1",
    "style-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "whatsapp-web.js": "^1.13.2",
    "workbox-webpack-plugin": "^6.1.5",
    "zip-local": "^0.3.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 15.8.0",
    "yarn": ">= 1.22.10",
    "npm": ">= 7.5.1"
  },
  "eslintConfig": {
    "extends": [
      "./devpackages/eslint-config"
    ]
  },
  "eslintIgnore": [
    "/**/dist",
    "/**/**/dist",
    "/**/build",
    "**/*.hbs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "domain-precommit-linter",
      "pre-push": "domain-prepush-linter"
    }
  },
  "preserveModulesImportPathName": []
}
